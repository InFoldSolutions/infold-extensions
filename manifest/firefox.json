{
	"manifest_version": 2,
	"name": "InFold",
	"description": "MAKING NEWS INFORMATIVE SINCE 2018",
	"version": "0.7.2",
	"permissions": [
		"https://*.infold.ai/*"
	],
	"icons": {
		"64": "images/icons/icon_64.png",
		"128": "images/icons/icon_128.png"
	},
	"content_security_policy": "img-src 'self' https://*.infold.ai/*; object-src 'self' https://*.infold.ai/*; script-src 'self' https://*.infold.ai/*;",
	"content_scripts": [
		{
			"matches": [
				"*://*.reddit.com/*",
				"*://*.twitter.com/*"
			],
			"js": [
				"app.js"
			],
			"css": [
				"css/all.min.css",
				"css/main.css"
			]
		}
	],
	"web_accessible_resources": [
		"images/*",
		"webfonts/*"
	]
}